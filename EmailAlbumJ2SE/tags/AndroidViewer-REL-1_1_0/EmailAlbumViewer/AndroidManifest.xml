<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.kg.emailalbum.mobile" android:versionCode="3" android:versionName="1.1.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name">

		<activity android:name=".EmailAlbumViewer" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>

			<intent-filter android:icon="@drawable/icon"
				android:label="EmailAlbumViewer">
				<action android:name="android.intent.action.VIEW"></action>
				<category android:name="android.intent.category.DEFAULT"></category>
				<data android:mimeType="application/java-archive"></data>
			</intent-filter>
			<intent-filter android:icon="@drawable/icon"
				android:label="EmailAlbumViewer">
				<action android:name="android.intent.action.VIEW"></action>
				<category android:name="android.intent.category.DEFAULT"></category>
				<data android:mimeType="application/zip"></data>
			</intent-filter>
		</activity>
		<activity android:name=".ShowPics"></activity>
		<activity android:name=".AboutDialog" android:theme="@android:style/Theme.Dialog">
		</activity>
	</application>
	<uses-sdk android:minSdkVersion="2" />


	<uses-permission android:name="android.permission.SET_WALLPAPER"></uses-permission>
</manifest> 